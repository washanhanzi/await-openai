	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'await-openai'",
			"cargo": {
				"args": [
					"test",
					"--lib",
					"--package=await-openai",
					"--package=await-openai",
					"--features=tiktoken"
				],
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug benchmark 'async-openai'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bench=async-openai",
					"--package=await-openai",
				],
				"filter": {
					"name": "async-openai",
					"kind": "bench"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}